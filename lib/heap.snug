

seq Heap
{
    size,
    buffer,
}



"(Buffer::Ptr, Size::Int) => (::Heap)
rout Heap::Create
{
    pull size;
    pull buffer;

    put buffer.Heap::buffer = buffer;
    put buffer.Heap::size = size - (Heap - 1);

    push buffer;
}



